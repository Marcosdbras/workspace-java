import java.util.Random;

public class Aplicacao {

	public static void main(String[] args) {
	  for (int i = 0; i < 10; i++) {
		  
		  double d = Math.random();
		  
		  System.out.println((i + 1) + " => " +d);
		  
		  
		  
	  }	
	  
	  
	  Random r = new Random();
	  
	  for (int i = 0;i < 10;i++) {
		  
		  
		  int dd = r.nextInt();
		  
		  System.out.println((i + 1)+" => "+dd);
		  
	  }

	}

}
