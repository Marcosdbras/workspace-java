
public class Endereco {
  private String logrador;
  private int numero;

  
  public String getLogrador() {
	return logrador;
  }

  
  public void setLogrador(String logrador) {
	this.logrador = logrador;
  }

  
  public int getNumero() {
	return numero;
  }

  
  public void setNumero(int numero) {
	this.numero = numero;
  }
  
  
}
