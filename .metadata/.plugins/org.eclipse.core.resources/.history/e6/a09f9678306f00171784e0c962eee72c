
public class Aplicacao {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Carro c = new Carro();
		c.setAno(2000);
		c.setModelo("Brasilia");
		c.setMarca("VW");
		c.setQuatroPortas(true);
		
		c.imprimirDados();
		c.buzinar();
		
		
		Motocicleta m = new Motocicleta();
		m.buzinar();
		
		Caminhao cam = new Caminhao();
		cam.buzinar();

	}

}
