package thread;

public class Contador extends Thread {
    
	/*
	 * CTRL + Space 
	 *    Auxilio de c√≥digo
	 * 
	 * */
	
	@Override
	public void run() {
		
		for (int i = 0; i < 10; i++) {
		
			System.out.println(" Contador Thread => "+i);
			
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				
				e.printStackTrace();
			}
			
		}
		
		
	}
	
	
	
}
